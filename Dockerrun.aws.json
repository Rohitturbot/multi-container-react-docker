{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions":[
    {
      "name":"client",
      "image":"rohityadav007/multi-client",
      "hostName":"client",
      "essential":false
    },
    {
      "name":"server",
      "image":"rohityadav007/multi-server",
      "hostName":"api",
      "essential":false
    },
    {
      "name":"worker",
      "image":"rohityadav007/multi-worker",
      "essential":false
    },
    {
      "name":"nginx",
      "image":"rohityadav007/multi-nginx",
      "essential":true,
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "links":["client", "server"]
    }
  ]
}